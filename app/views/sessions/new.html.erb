<h1><%= t ".Log in" %></h1>
<br>
<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
  <div>
    <%= f.label t ".email" %><br>
    <%= f.email_field :email, placeholder: "メールアドレス" %><br>
  </div>
  <br>
  <div>
    <%= f.label t ".password" %><br>
    <%= f.password_field :password, placeholder: "パスワード" %>
  </div>
  <br>
  <%= f.submit "ログイン", class: 'btn btn-primary' %>
<% end %>